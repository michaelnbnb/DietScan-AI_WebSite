<svg width="250" height="200" viewBox="0 0 250 200" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="step3Gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f59e0b;stop-opacity:0.1" />
      <stop offset="100%" style="stop-color:#d97706;stop-opacity:0.1" />
    </linearGradient>
    <linearGradient id="chartGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#4f46e5;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#7c3aed;stop-opacity:0.4" />
    </linearGradient>
  </defs>
  
  <!-- 背景 -->
  <rect width="250" height="200" rx="12" fill="url(#step3Gradient)" stroke="#f59e0b" stroke-width="2" stroke-dasharray="5,5"/>
  
  <!-- 手机轮廓 -->
  <rect x="80" y="20" width="90" height="140" rx="15" fill="#1f2937" stroke="#374151" stroke-width="2"/>
  
  <!-- 屏幕 -->
  <rect x="90" y="35" width="70" height="110" rx="8" fill="white"/>
  
  <!-- 营养分析结果界面 -->
  <g transform="translate(95, 40)">
    <!-- 标题 -->
    <text x="30" y="15" font-family="Arial, sans-serif" font-size="10" font-weight="bold" fill="#1f2937" text-anchor="middle">营养分析结果</text>
    
    <!-- 卡路里显示 -->
    <rect x="5" y="20" width="50" height="25" rx="4" fill="#4f46e5" opacity="0.1"/>
    <text x="30" y="30" font-family="Arial, sans-serif" font-size="8" fill="#4f46e5" text-anchor="middle">卡路里</text>
    <text x="30" y="40" font-family="Arial, sans-serif" font-size="10" font-weight="bold" fill="#1f2937" text-anchor="middle">350</text>
    
    <!-- 营养成分条形图 -->
    <g transform="translate(0, 50)">
      <!-- 蛋白质 -->
      <text x="0" y="8" font-family="Arial, sans-serif" font-size="7" fill="#6b7280">蛋白质</text>
      <rect x="0" y="10" width="60" height="4" rx="2" fill="#e5e7eb"/>
      <rect x="0" y="10" width="40" height="4" rx="2" fill="#10b981"/>
      <text x="45" y="13" font-family="Arial, sans-serif" font-size="6" fill="#10b981">25g</text>
      
      <!-- 碳水化合物 -->
      <text x="0" y="23" font-family="Arial, sans-serif" font-size="7" fill="#6b7280">碳水</text>
      <rect x="0" y="25" width="60" height="4" rx="2" fill="#e5e7eb"/>
      <rect x="0" y="25" width="50" height="4" rx="2" fill="#3b82f6"/>
      <text x="52" y="28" font-family="Arial, sans-serif" font-size="6" fill="#3b82f6">45g</text>
      
      <!-- 脂肪 -->
      <text x="0" y="38" font-family="Arial, sans-serif" font-size="7" fill="#6b7280">脂肪</text>
      <rect x="0" y="40" width="60" height="4" rx="2" fill="#e5e7eb"/>
      <rect x="0" y="40" width="20" height="4" rx="2" fill="#f59e0b"/>
      <text x="22" y="43" font-family="Arial, sans-serif" font-size="6" fill="#f59e0b">12g</text>
    </g>
  </g>
  
  <!-- 数据可视化图表 -->
  <g transform="translate(30, 60)">
    <!-- 饼图 -->
    <circle cx="0" cy="0" r="20" fill="none" stroke="#e5e7eb" stroke-width="4"/>
    <circle cx="0" cy="0" r="20" fill="none" stroke="#4f46e5" stroke-width="4" 
            stroke-dasharray="31.4 94.2" stroke-dashoffset="0" transform="rotate(-90)"/>
    <circle cx="0" cy="0" r="20" fill="none" stroke="#10b981" stroke-width="4" 
            stroke-dasharray="25.1 100.5" stroke-dashoffset="-31.4" transform="rotate(-90)"/>
    <circle cx="0" cy="0" r="20" fill="none" stroke="#f59e0b" stroke-width="4" 
            stroke-dasharray="18.8 106.8" stroke-dashoffset="-56.5" transform="rotate(-90)"/>
  </g>
  
  <!-- 成功图标 -->
  <g transform="translate(200, 60)">
    <circle cx="0" cy="0" r="15" fill="#10b981"/>
    <path d="M-6 0 L-2 4 L6 -4" stroke="white" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
  </g>
  
  <!-- 文字说明 -->
  <text x="125" y="185" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#1f2937" text-anchor="middle">获取详细结果</text>
</svg>