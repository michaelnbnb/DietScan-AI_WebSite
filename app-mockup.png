<svg width="300" height="600" viewBox="0 0 300 600" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="phoneGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8fafc;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e2e8f0;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="screenGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- 手机外框 -->
  <rect x="20" y="20" width="260" height="560" rx="30" fill="#1f2937" stroke="#374151" stroke-width="2"/>
  
  <!-- 屏幕 -->
  <rect x="35" y="60" width="230" height="480" rx="20" fill="url(#screenGradient)"/>
  
  <!-- 状态栏 -->
  <rect x="35" y="60" width="230" height="30" rx="20" fill="rgba(0,0,0,0.3)"/>
  <text x="50" y="78" font-family="Arial, sans-serif" font-size="12" fill="white">9:41</text>
  <circle cx="240" cy="75" r="3" fill="white"/>
  <rect x="245" y="72" width="15" height="6" rx="2" fill="white"/>
  
  <!-- 应用界面内容 -->
  <g transform="translate(35, 90)">
    <!-- 标题栏 -->
    <rect x="0" y="0" width="230" height="50" fill="rgba(255,255,255,0.1)"/>
    <text x="15" y="30" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="white">食摄营养师</text>
    
    <!-- 拍照区域 -->
    <rect x="15" y="70" width="200" height="150" rx="10" fill="rgba(255,255,255,0.9)"/>
    <circle cx="115" cy="145" r="30" fill="#4f46e5"/>
    <path d="M105 145 L115 135 L125 145 L115 155 Z" fill="white"/>
    <text x="115" y="175" font-family="Arial, sans-serif" font-size="12" fill="#6b7280" text-anchor="middle">拍摄食物照片</text>
    
    <!-- 营养信息卡片 -->
    <rect x="15" y="240" width="200" height="80" rx="8" fill="rgba(255,255,255,0.9)"/>
    <text x="25" y="260" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#1f2937">营养分析</text>
    <text x="25" y="280" font-family="Arial, sans-serif" font-size="12" fill="#6b7280">卡路里: 350 kcal</text>
    <text x="25" y="295" font-family="Arial, sans-serif" font-size="12" fill="#6b7280">蛋白质: 25g</text>
    <text x="25" y="310" font-family="Arial, sans-serif" font-size="12" fill="#6b7280">碳水: 45g</text>
    
    <!-- 进度条 -->
    <rect x="15" y="340" width="200" height="40" rx="8" fill="rgba(255,255,255,0.9)"/>
    <text x="25" y="355" font-family="Arial, sans-serif" font-size="12" fill="#1f2937">今日目标进度</text>
    <rect x="25" y="365" width="180" height="6" rx="3" fill="#e5e7eb"/>
    <rect x="25" y="365" width="120" height="6" rx="3" fill="#4f46e5"/>
    
    <!-- 底部导航 -->
    <rect x="0" y="400" width="230" height="50" fill="rgba(255,255,255,0.1)"/>
    <circle cx="57" cy="425" r="8" fill="white"/>
    <circle cx="115" cy="425" r="8" fill="rgba(255,255,255,0.5)"/>
    <circle cx="173" cy="425" r="8" fill="rgba(255,255,255,0.5)"/>
  </g>
  
  <!-- Home 指示器 -->
  <rect x="130" y="560" width="40" height="4" rx="2" fill="#9ca3af"/>
</svg>